<script>
//imports
name = 'PrimaryMenu';
import MenuData from '~/static/OrderMenus.json';
export default {
  data: function () {
    return {
      items: [],
    };
  },
  created() {
    this.items = MenuData.map(GetMenuTitles);
    function GetMenuTitles(item) {
      let link = '/menu/' + item.Name.replace(/\W/g, '_');
      return { title: item.Name, link: link };
    }
  },
};
</script>

